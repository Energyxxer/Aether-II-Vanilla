@ tag tick

at @a function {
    if block ~ ~-.1 ~ aether:quicksoil summon $pusher
    if block ~ ~-.1 ~ aether:quicksoil summon $pusher
    fill ~-5 ~-5 ~-5 ~5 ~5 ~5 minecraft:jungle_leaves[persistent=true] replace minecraft:dark_oak_sapling[stage=1]
}

